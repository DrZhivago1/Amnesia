<?xml version="1.0" encoding="UTF-8"?>
<Patch>
   <!--Remove all starting research from "New Arrivals" faction-->
   <Operation Class="PatchOperationSequence">
      <success>Always</success>
      <operations>
         <!--ResearchProjects_1.xml remove research for "New Arrivals"-->
         <li Class="PatchOperationRemove">
            <xpath>Defs/ResearchProjectDef[defName="ComplexFurniture" or defName="PassiveCooler" or defName="Stonecutting" or defName="ComplexClothing"]/tags/li[.="ClassicStart"]</xpath>
         </li>
         <!--ResearchProjects_2_Electricity.xml remove research for "New Arrivals"-->
         <li Class="PatchOperationRemove">
            <xpath>Defs/ResearchProjectDef[defName="Electricity" or defName="NutrientPaste" or defName="AirConditioning"]/tags/li[.="ClassicStart"]</xpath>
         </li>
      </operations>
   </Operation>
</Patch>