<?xml version="1.0" encoding="UTF-8"?>
<Patch>
    <!--If using Medical System Expansion, Remove all starting research from "New Arrivals" faction-->
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>Medical System Expansion</li>
        </mods>
        <match Class="PatchOperationSequence">
            <success>Always</success>
            <operations>
                <li Class="PatchOperationRemove">
                    <xpath>Defs/ResearchProjectDef[defName="MSE_BasicProsthetics"]/tags/li[.="ClassicStart"]</xpath>
                </li>
            </operations>
        </match>
    </Operation>
</Patch>