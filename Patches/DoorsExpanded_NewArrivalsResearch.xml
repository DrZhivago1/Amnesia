<?xml version="1.0" encoding="UTF-8"?>
<Patch>
   <!--If using Doors Expanded, Remove all starting research from "New Arrivals" faction-->
   <Operation Class="PatchOperationFindMod">
      <mods>
         <li>Doors Expanded</li>
      </mods>
      <match Class="PatchOperationSequence">
         <success>Always</success>
         <operations>
            <!--Heron_ResearchProjects.xml remove research for "New Arrivals"-->
            <li Class="PatchOperationRemove">
               <xpath>Defs/ResearchProjectDef[defName="ProjectHeron_Curtains" or defName="ProjectHeron_Gates"]/tags/li[.="ClassicStart"]</xpath>
            </li>
         </operations>
      </match>
   </Operation>
</Patch>