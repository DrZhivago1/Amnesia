<?xml version="1.0" encoding="UTF-8"?>
<Patch>
   <!--If using Apparello 2, Remove all starting research from "New Arrivals" faction-->
   <Operation Class="PatchOperationFindMod">
      <mods>
         <li>Apparello 2</li>
      </mods>
      <match Class="PatchOperationSequence">
         <success>Always</success>
         <operations>
            <!--Arabatschupp.xml remove research for "New Arrivals"-->
            <li Class="PatchOperationRemove">
               <xpath>Defs/ResearchProjectDef[defName="ApparelloResearchQuilting"]/tags/li[.="ClassicStart"]</xpath>
            </li>
         </operations>
      </match>
   </Operation>
</Patch>