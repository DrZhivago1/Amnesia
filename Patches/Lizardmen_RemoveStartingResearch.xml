<?xml version="1.0" encoding="UTF-8"?>
<Patch>
   <!--Remove all starting research from "Lizardmen" faction-->
   <Operation Class="PatchOperationFindMod">
      <mods>
         <li>Lizardmen</li>
      </mods>
      <match Class="PatchOperationRemove">
         <xpath>/Defs/FactionDef[defName="PlayerLizardmanColony"]/startingResearchTags</xpath>
      </match>
   </Operation>
</Patch>