<?xml version="1.0" encoding="UTF-8"?>
<Patch>
    <!--If using RIMkea, Remove all starting research from "New Tribe" faction-->
    <Operation Class="PatchOperationFindMod">
        <mods>
            <li>RIMkea</li>
        </mods>
        <match Class="PatchOperationSequence">
            <success>Always</success>
            <operations>
                <!--Research_RIMkea.xml remove research for "New Tribe"-->
                <li Class="PatchOperationRemove">
                    <xpath>Defs/researchProjectDef[defName="RustikaFurniture"]/tags/li[.="TribalStart"]</xpath>
                </li>
            </operations>
        </match>
    </Operation>
</Patch>